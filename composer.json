{
    "name": "pyaesoneaung/to-raw-sql",
    "description": "Get raw SQL from Laravel Query Builder",
    "keywords": [
        "pyaesoneaung",
        "laravel",
        "to-raw-sql"
    ],
    "homepage": "https://github.com/PyaeSoneAungRgn/to-raw-sql",
    "license": "MIT",
    "authors": [
        {
            "name": "Pyae Sone Aung",
            "email": "pyaesoneaungrgn@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0|^8.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "PyaeSoneAung\\ToRawSql\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PyaeSoneAung\\ToRawSql\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PyaeSoneAung\\ToRawSql\\ToRawSqlServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}